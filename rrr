task.spawn(function()
    while true do
        for Index, Coin in pairs(CoinTable) do
            Coin.model:Destroy()
            Coin.followPart:Destroy()
            table.remove(CoinTable, Index)
            CoinEvent:FireServer(Coin.id)
        end
        task.wait()
    end
end)

